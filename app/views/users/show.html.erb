<% provide(:title, @user.name) %>
<div class="row">
  <aside class="span4">
    <section>
      <h1>
        <%= @user.name %>
      </h1>
    </section>
  </aside>
   <div class="span8">
    <% if @user.bookings.any? %>
      <h3>events (<%= @user.bookings.count %>)</h3>
      <ol class="events">
        <%#= render @bookings %>
      </ol>
      <%#= will_paginate @bookings %>
    <% end %>
  </div>
</div>